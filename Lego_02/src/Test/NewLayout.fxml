<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<top>
		<VBox BorderPane.alignment="CENTER">
			<children>
				<MenuBar>
					<menus>
						<Menu mnemonicParsing="false" text="File">
							<items>
								<MenuItem mnemonicParsing="false" text="New">
									<accelerator>
										<KeyCodeCombination alt="UP" code="N"
											control="DOWN" meta="UP" shift="UP" shortcut="UP" />
									</accelerator>
								</MenuItem>
								<MenuItem mnemonicParsing="false" text="Open">
									<accelerator>
										<KeyCodeCombination alt="UP" code="O"
											control="DOWN" meta="UP" shift="UP" shortcut="UP" />
									</accelerator>
								</MenuItem>
								<MenuItem mnemonicParsing="false" text="Save">
									<accelerator>
										<KeyCodeCombination alt="UP" code="S"
											control="DOWN" meta="UP" shift="UP" shortcut="UP" />
									</accelerator>
								</MenuItem>
								<MenuItem mnemonicParsing="false" text="Exit">
									<accelerator>
										<KeyCodeCombination alt="UP" code="Q"
											control="DOWN" meta="UP" shift="UP" shortcut="UP" />
									</accelerator>
								</MenuItem>
							</items>
						</Menu>
						<Menu mnemonicParsing="false" text="Edit">
							<items>
								<MenuItem mnemonicParsing="false" text="Undo">
									<accelerator>
										<KeyCodeCombination alt="UP" code="Z"
											control="DOWN" meta="UP" shift="UP" shortcut="UP" />
									</accelerator>
								</MenuItem>
								<MenuItem mnemonicParsing="false" text="Redo">
									<accelerator>
										<KeyCodeCombination alt="UP" code="Y"
											control="DOWN" meta="UP" shift="UP" shortcut="UP" />
									</accelerator>
								</MenuItem>
							</items>
						</Menu>
						<Menu mnemonicParsing="false" text="Help">
							<items>
								<MenuItem mnemonicParsing="false" text="Help" />
								<MenuItem mnemonicParsing="false" text="About" />
								<MenuItem mnemonicParsing="false" text="Privacy Policy" />
							</items>
						</Menu>
					</menus>
				</MenuBar>
				<ToolBar prefHeight="40.0" prefWidth="200.0">
					<items>
						<Button fx:id="btnHome" mnemonicParsing="false">
							<graphic>
								<ImageView fitHeight="48.0" fitWidth="48.0"
									pickOnBounds="true" preserveRatio="true">
									<image>
										<Image url="@../home.png" />
									</image>
									<viewport>
										<Rectangle2D />
									</viewport>
								</ImageView>
							</graphic>
						</Button>
						<Button contentDisplay="CENTER" mnemonicParsing="false">
							<graphic>
								<ImageView pickOnBounds="true" preserveRatio="true">
									<image>
										<Image url="@../save.png" />
									</image>
								</ImageView>
							</graphic>
						</Button>
						<Button graphicTextGap="0.0" mnemonicParsing="false">
							<graphic>
								<ImageView pickOnBounds="true" preserveRatio="true">
									<image>
										<Image url="@../undo.png" />
									</image>
								</ImageView>
							</graphic>
						</Button>
						<Button mnemonicParsing="false" prefHeight="56.0"
							prefWidth="37.0">
							<graphic>
								<ImageView fitHeight="48.0" fitWidth="48.0"
									pickOnBounds="true" preserveRatio="true">
									<image>
										<Image url="@../redo.png" />
									</image>
								</ImageView>
							</graphic>
						</Button>
					</items>
				</ToolBar>
			</children>
		</VBox>
	</top>
</BorderPane>